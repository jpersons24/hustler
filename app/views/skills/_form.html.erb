<% if flash[:errors] %>
   <% flash[:errors].each do |e| %>
      < p style="color:red;"><%= e %></p>
   <% end %>
<%= end %>

<%= form_for @skill do |f| %>
   <%= f.label "Name:" %>
   <%= f.text_field :name %> <br>
   <%= f.label "Category:" %>
   <%= f.text_field :category %> <br>
   <%= f.submit %>
<% end %>